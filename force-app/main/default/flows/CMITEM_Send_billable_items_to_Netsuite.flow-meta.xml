    <formulas>
        <name>varB_MissingInfoAccount</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!Get_Account.RegistrationNumber__c}) ||
ISBLANK({!Get_Account.VATNumber__c}) ||
ISBLANK(TEXT({!Get_Account.Activity_Country__c})) ||
(
   ISBLANK({!Get_Account.ID_interne_TdG__c}) &amp;&amp;
   TEXT({!Get_Account.Sous_type__c}) = &apos;TSC&apos; &amp;&amp;
   TEXT({!Get_Charge_Map.SourceOpportunity__r.Activity__c}) = &apos;Pixid FR&apos; &amp;&amp;
   TEXT({!Get_Charge_Map.SourceOpportunity__r.Offer__c}) = &apos;Pack&apos;
)</expression>
    </formulas>
    <formulas>
        <name>varB_MissingInfoChargeMap</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!Get_Charge_Map.BillingContact__c}) 
|| ISBLANK({!Get_Charge_Map.CashCollectionContact__c}) 
|| ISBLANK(TEXT({!Get_Charge_Map.PaymentMethod__c})) 
|| ISBLANK(TEXT({!Get_Charge_Map.BillingConditions__c})) 
|| ISBLANK(TEXT({!Get_Charge_Map.InvoiceSendingMethod__c})) 
|| ISBLANK({!Get_Charge_Map.BillingInstructions__c})  
|| ISBLANK({!Get_Charge_Map.ContractDurationInYears__c})  
|| (
    TEXT({!Get_Charge_Map.InvoiceSendingMethod__c}) = &quot;Email&quot; 
    &amp;&amp; ISBLANK({!Get_Charge_Map.SendInvoiceTo__c})
) 
|| (
    TEXT({!Get_Charge_Map.InvoiceSendingMethod__c}) = &quot;Platform&quot;
    &amp;&amp; ISBLANK({!Get_Charge_Map.PlatformLink__c})
)</expression>
    </formulas>
    <formulas>
        <name>varF_TodaysDate</name>
